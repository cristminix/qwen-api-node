{
  "name": "@qwen-api/client",
  "version": "0.1.0",
  "description": "A Node.js client for the Qwen API.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "esbuild --minify --bundle src/index.ts --outdir=build --platform=node",
    "prepublishOnly": "npm run build",
    "dev": "tsx watch src/index.ts",
    "start": "node build/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "keywords": [
    "qwen",
    "api",
    "ai",
    "llm"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/ali-oss": "^6.16.11",
    "@types/node": "^24.0.15",
    "dotenv": "^17.0.1",
    "drizzle-kit": "^0.31.4",
    "esbuild": "^0.25.8",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@hono/node-server": "^1.16.0",
    "ali-oss": "^6.23.0",
    "axios": "^1.7.2",
    "better-sqlite3": "^12.2.0",
    "cheerio": "^1.1.2",
    "drizzle-orm": "^0.44.4",
    "fast-crc32c": "^2.0.0",
    "hono": "^4.8.5",
    "openai": "^6.1.0",
    "proxy-agent": "^6.5.0",
    "tiktoken": "^1.0.22",
    "turndown": "^7.2.1",
    "uuid": "^11.1.0"
  }
}
